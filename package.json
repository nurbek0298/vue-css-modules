{
  "name": "vue-css-modules",
  "displayName": "Vue CSS Modules",
  "description": "Extension for quick HTML markup creation in Vue for CSS modules.",
  "publisher": "nurbek0298",
  "version": "0.0.2",
  "icon": "./docs/logo.png",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nurbek0298/vue-css-modules"
  },
  "keywords": [
    "vue",
    "css modules",
    "emmet",
    "vue css modules",
    "vue emmet css modules",
    "vue css modules snippets",
    "vue css modules emmet",
    "vue css modules emmet snippets"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "vsce:package": "vsce package --no-dependencies",
    "vcse:publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
